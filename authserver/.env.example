# Google OAuth (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# OpenRouter provisioning key (get from OpenRouter dashboard)
# This key is used to CREATE new keys, not for API calls
OPENROUTER_PROVISIONING_KEY=sk-or-v1-xxx

# Per-user credit limit in USD
OPENROUTER_CREDIT_LIMIT=1.0

# Cartesia API key (shared across all users)
CARTESIA_API_KEY=your-cartesia-key

# Vercel KV (auto-populated when you connect KV storage)
# KV_REST_API_URL=
# KV_REST_API_TOKEN=
